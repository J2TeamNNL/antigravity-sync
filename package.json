{
  "name": "ai-context-sync",
  "displayName": "AI Context Sync",
  "description": "Sync AI context & settings across multiple IDEs (Cursor, Windsurf, VS Code). Private, Git-based, zero babysitting.",
  "version": "2.0.0",
  "publisher": "j2teamnnl",
  "author": "Nguyen Nam Long <https://www.facebook.com/j2teamnnl>",
  "icon": "resources/icons/icon.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/j2teamnnl/ai-context-sync"
  },
  "bugs": {
    "url": "https://github.com/j2teamnnl/ai-context-sync/issues"
  },
  "homepage": "https://github.com/j2teamnnl/antigravity-sync#readme",
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "ai context sync",
    "ai agent automation"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiContextSync.configure",
        "title": "AI Context Sync: Configure Repository",
        "icon": "$(gear)"
      },
      {
        "command": "aiContextSync.syncNow",
        "title": "AI Context Sync: Sync Now",
        "icon": "$(sync)"
      },
      {
        "command": "aiContextSync.push",
        "title": "AI Context Sync: Push Changes",
        "icon": "$(cloud-upload)"
      },
      {
        "command": "aiContextSync.pull",
        "title": "AI Context Sync: Pull Changes",
        "icon": "$(cloud-download)"
      },
      {
        "command": "aiContextSync.showStatus",
        "title": "AI Context Sync: Show Status",
        "icon": "$(info)"
      },
      {
        "command": "aiContextSync.openPanel",
        "title": "AI Context Sync: Open Panel",
        "icon": "$(window)"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "ai-context-sync",
          "title": "AI Context Sync",
          "icon": "resources/icons/sidebar.svg"
        }
      ]
    },
    "views": {
      "ai-context-sync": [
        {
          "type": "webview",
          "id": "aiContextSync.mainPanel",
          "name": "Sync Dashboard"
        }
      ]
    },
    "configuration": {
      "title": "AI Context Sync",
      "properties": {
        "aiContextSync.repositoryUrl": {
          "type": "string",
          "default": "",
          "description": "GitHub repository URL for syncing (must be private)"
        },
        "aiContextSync.enabled": {
          "type": "boolean",
          "default": true,
          "description": "Enable or disable AI Context Sync completely"
        },
        "aiContextSync.autoSync": {
          "type": "boolean",
          "default": true,
          "description": "Automatically sync changes when files change"
        },
        "aiContextSync.syncIntervalMinutes": {
          "type": "number",
          "default": 5,
          "minimum": 1,
          "maximum": 60,
          "description": "Auto-sync interval in minutes"
        },
        "aiContextSync.excludePatterns": {
          "type": "array",
          "default": [],
          "items": {
            "type": "string"
          },
          "description": "Additional patterns to exclude from sync"
        },
        "aiContextSync.geminiPath": {
          "type": "string",
          "default": "",
          "description": "Custom path to .gemini folder (default: ~/.gemini)"
        },
        "aiContextSync.syncFolders": {
          "type": "array",
          "default": [
            "knowledge",
            "brain"
          ],
          "items": {
            "type": "string"
          },
          "description": "Folders to sync within .gemini directory"
        },
        "aiContextSync.enabledAgents": {
          "type": "array",
          "default": [
            "antigravity"
          ],
          "items": {
            "type": "string",
            "enum": [
              "antigravity",
              "cursor",
              "windsurf"
            ]
          },
          "description": "Enabled AI agents for sync"
        },
        "aiContextSync.syncMode": {
          "type": "string",
          "default": "private",
          "enum": [
            "private"
          ],
          "description": "Sync mode: global only"
        },
        "aiContextSync.agentPaths": {
          "type": "object",
          "default": {},
          "description": "Per-agent path settings (global/project enabled, custom global path)"
        },
        "aiContextSync.agentExcludePatterns": {
          "type": "object",
          "default": {},
          "description": "Per-agent exclude patterns (merged with global excludes)"
        }
      }
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "build": "webpack --mode production",
    "build:dev": "webpack --mode development",
    "watch": "webpack --mode development --watch",
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --ext ts --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "vscode-test",
    "package": "vsce package --no-yarn",
    "publish": "vsce publish --no-yarn"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@types/vscode": "^1.85.0",
    "@types/vscode-webview": "^1.57.4",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vscode/test-electron": "^2.3.8",
    "@vscode/vsce": "^2.22.0",
    "copy-webpack-plugin": "^11.0.0",
    "css-loader": "^6.8.1",
    "eslint": "^8.54.0",
    "jest": "^29.7.0",
    "style-loader": "^3.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@octokit/rest": "^20.0.2",
    "@vscode/webview-ui-toolkit": "^1.4.0",
    "chokidar": "^3.5.3",
    "ignore": "^5.3.0",
    "simple-git": "^3.21.0",
    "ws": "^8.14.0"
  }
}
